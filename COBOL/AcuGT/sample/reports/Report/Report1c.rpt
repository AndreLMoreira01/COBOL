              END-PERFORM
              IF DUMY-LINE(ACU-IDX2 - 2:1) = "\"
                 STRING DUMY-LINE, PRINT-FULLNAME, X"00"
                    DELIMITED BY "*" INTO PRT-FULLFILENAME
