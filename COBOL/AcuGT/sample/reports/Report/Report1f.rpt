       Acu-Report1f-PRINT-PARA.
           MOVE SPACES TO PRINT-FULLNAME
           CALL "C$FULLNAME" USING "Report1f.html", PRINT-FULLNAME,
                GIVING ACU-STATUS-CODE
           IF ACU-STATUS-CODE = 1
              MOVE 0 TO ACU-SETUPPRINT
              EXIT PARAGRAPH
           END-IF
           MOVE 0 TO ACU-IDX1
           INSPECT PRINT-FULLNAME TALLYING ACU-IDX1 FOR ALL "\"
           INSPECT PRINT-FULLNAME REPLACING TRAILING SPACE BY "*"
           IF ACU-IDX1 <= 0
              MOVE SPACES TO DUMY-LINE
              CALL "C$CHDIR" USING DUMY-LINE
              INSPECT DUMY-LINE REPLACING TRAILING SPACE BY "*"
              MOVE 0 TO ACU-STATUS-CODE
              PERFORM VARYING ACU-IDX2 FROM 1 BY 1 UNTIL
              ACU-STATUS-CODE = 1
                 IF DUMY-LINE(ACU-IDX2:1) = "*"
                    MOVE 1 TO ACU-STATUS-CODE
                 END-IF
              END-PERFORM
              IF DUMY-LINE(ACU-IDX2 - 2:1) = "\"
                 STRING DUMY-LINE, PRINT-FULLNAME, X"00"
                    DELIMITED BY "*" INTO PRT-FULLFILENAME
              ELSE
                 STRING DUMY-LINE, "\", PRINT-FULLNAME, X"00"
                    DELIMITED BY "*" INTO PRT-FULLFILENAME
              END-IF
           ELSE
              STRING PRINT-FULLNAME , X"00" DELIMITED BY "*" INTO 
                 PRT-FULLFILENAME
           END-IF

           STRING "Letter", X"00" DELIMITED BY SIZE INTO 
              ACU-PAPER-SIZE
           STRING "&w&bPage &p of &P", X"00" DELIMITED BY SIZE INTO 
              ACU-PAPER-HEADER
           STRING "&u&b&d", X"00" DELIMITED BY SIZE INTO 
              ACU-PAPER-FOOTER
           MOVE "PORTRAIT" TO ACU-PAPER-ORIENTATION
           IF ACU-PAPER-ORIENTATION = "LANDSCAPE"
              MOVE 2 TO ACU-PAPER-ORIENTATIONINT
           ELSE
              MOVE 1 TO ACU-PAPER-ORIENTATIONINT
           END-IF
           STRING "PORTRAIT", X"00" DELIMITED BY SIZE INTO
              ACU-PAPER-ORIENTATION
           MOVE "75" to ACU-MARGIN-LEFT
           MOVE "75" to ACU-MARGIN-RIGHT
           MOVE "75" to ACU-MARGIN-TOP
           MOVE "75" to ACU-MARGIN-BOTTOM
           STRING ACU-PAPER-MARGIN, X"00" DELIMITED BY SIZE INTO 
              PRINT-BROWSER-PARA
           IF ACU-PAPER-COPIES = 0 
              OR ACU-PAPER-COPIES(1:) = spaces
                 MOVE 1 to ACU-PAPER-COPIES
           END-IF
           MOVE SPACES TO DUMY-LINE
           .

       Acu-Report1f-SETUP-PRINT.
           MOVE 1 TO ACU-SETUPPRINT
           PERFORM Acu-Report1f-PRINT-PARA
           SET ENVIRONMENT "DLL_CONVENTION" to "1"
           CALL "AcuBenchPrintDummy"
              ON EXCEPTION CALL "AcuBenchPrint.dll" END-CALL
           END-CALL
           CALL "AcuBenchPrintExecWBPageSetup" USING
                BY CONTENT PRINT-BROWSER-PARA,
                BY CONTENT ACU-PAPER-SIZE, 
                BY VALUE   ACU-PAPER-ORIENTATIONINT,
                BY CONTENT ACU-PRINTER-NAME
                GIVING ACUPRINTDLL-STATUS
           END-CALL
           IF ACUPRINTDLL-SUCCESS
              MOVE 1 TO PRINT-UNIT-TYPE
              SET PRINT-PTR TO ADDRESS OF ACU-PAPER-MARGIN
              CALL "AcuBenchPrintGetMargins" USING BY 
                 VALUE PRINT-UNIT-TYPE, PRINT-PTR
              SET PRINT-PTR TO ADDRESS OF ACU-PAPER-SIZEINT
              CALL "AcuBenchPrintGetPaperSize" USING BY 
                 VALUE PRINT-UNIT-TYPE, PRINT-PTR
              COMPUTE PAGE-HEIGHT OF Report1f-HEIGHT = 
                 ACU-PAPER-HEIGHT - ACU-MARGIN-TOP 
                 - ACU-MARGIN-BOTTOM
              COMPUTE PAGE-WIDTH OF Report1f-HEIGHT = 
                 ACU-PAPER-WIDTH - ACU-MARGIN-LEFT
                 - ACU-MARGIN-RIGHT

              MOVE 2 TO PRINT-UNIT-TYPE
              SET PRINT-PTR TO ADDRESS OF ACU-PAPER-MARGIN
              CALL "AcuBenchPrintGetMargins" USING BY VALUE
                 PRINT-UNIT-TYPE, PRINT-PTR
              STRING ACU-PAPER-MARGIN, X"00" DELIMITED BY SIZE INTO
                 PRINT-BROWSER-PARA

              SET PRINT-PTR TO ADDRESS OF ACU-PAPER-SIZE
              CALL "AcuBenchPrintGetPaperName" USING BY VALUE PRINT-PTR
              CALL "AcuBenchPrintGetOrientation" GIVING 
                 ACU-PAPER-ORIENTATIONINT
              CALL "AcuBenchPrintGetCopies" GIVING ACU-PAPER-COPIES
              SET PRINT-PTR TO ADDRESS OF ACU-PRINTER-NAME
              CALL "AcuBenchPrintGetPrinterName" USING BY VALUE 
                 PRINT-PTR

              PERFORM Acu-Report1f-PRINT-TOFILE
              PERFORM Acu-Report1f-DO-PRINT
           END-IF
           .

       Acu-Report1f-PRINT.
           MOVE 0 TO ACU-SETUPPRINT
           PERFORM Acu-Report1f-PRINT-TOFILE
           PERFORM Acu-Report1f-DO-PRINT
           .

       Acu-Report1f-DO-PRINT.
           IF ACU-SETUPPRINT = 0
              PERFORM Acu-Report1f-PRINT-PARA
              SET ENVIRONMENT "DLL_CONVENTION" to "1"
              CALL "AcuBenchPrintDummy"
                 ON EXCEPTION CALL "AcuBenchPrint.dll" END-CALL
              END-CALL
           END-IF
           CALL "AcuBenchPrintExecWBPrint" USING
                BY CONTENT PRT-FULLFILENAME,
                OMITTED,
                BY CONTENT PRINT-BROWSER-PARA,
                BY CONTENT ACU-PAPER-HEADER, 
                BY CONTENT ACU-PAPER-FOOTER, 
                BY CONTENT ACU-PAPER-SIZE, 
                BY VALUE ACU-PAPER-ORIENTATIONINT,
                BY VALUE ACU-PAPER-COPIES,
                BY CONTENT ACU-PRINTER-NAME
           END-CALL
           MOVE 0 TO ACU-SETUPPRINT
           .

       Acu-Report1f-PREVIEW.
           PERFORM Acu-Report1f-PRINT-TOFILE
           PERFORM Acu-Report1f-PRINT-PARA
           SET ENVIRONMENT "DLL_CONVENTION" to "1"
           CALL "AcuBenchPrintDummy"
              ON EXCEPTION CALL "AcuBenchPrint.dll" END-CALL
           END-CALL
           CALL "AcuBenchPrintExecWBPreview" USING
                BY CONTENT PRT-FULLFILENAME,
                BY CONTENT PRINT-BROWSER-PARA,
                BY CONTENT ACU-PAPER-HEADER, 
                BY CONTENT ACU-PAPER-FOOTER, 
                BY CONTENT ACU-PAPER-SIZE, 
                BY VALUE ACU-PAPER-ORIENTATIONINT,
                BY CONTENT ACU-PRINTER-NAME
           END-CALL
           .

       Acu-Report1f-PRINT-TOFILE.
           MOVE "Report1f.html" TO PTR-DEV-NAME.
           MOVE 1 TO CURRENT-COL
           MOVE 0 TO Report1f-CLEFTOFFSET
           MOVE 0 TO CNT-PIXELS
           MOVE 1 TO NTOPCOLUMNNUM
           PERFORM Acu-PRINTF-OPEN-ROUTINE.
      *    BeforePrint           
           PERFORM Acu-Report1f-HTML-HEADER.
           PERFORM Acu-Report1f-HTML-PAGE-HEADER.
           PERFORM Acu-RPT-Report1f-PRINT-LOOP.
           PERFORM Acu-Report1f-HTML-FOOTER.
      *    AfterPrint
           PERFORM Acu-PRINTF-CLOSE-ROUTINE
           .

       Acu-RPT-Report1f-PRINT-LOOP.
           MOVE 1 TO TMP-PRINTORNOT
           MOVE 0 TO CNT-INPAGEPIXELS
           MOVE 1 TO ACU-PAGENUM
           MOVE 1 TO ACU-PAGENUM-SYS
           MOVE 0 TO SYS-ENDPRIMARYFD
           MOVE 1 TO REPORT-FOOTER-PRINT-AFTER-PAGE
           PERFORM Acu-RPT-Report1f-ALL-RESET
      * Check Report Header
           PERFORM Acu-RPT-Report1f-REPORT-HEADER
      * Start Normal (Page H/F, Group H/F, Detail)
           PERFORM Acu-RPT-Report1f-MASTER-PRINT-LOOP
      * End Normal (Page H/F, Group H/F, Detail)
           PERFORM Acu-RPT-Report1f-PAGE-HEADER
      * Check Last Group Footer
           PERFORM Acu-RPT-Report1f-GROUP-MUSTDSP
           PERFORM Acu-RPT-Report1f-GROUP-FOOTER
      * Check Report Footer
           IF ( REPORT-FOOTER-PRINT-AFTER-PAGE = 0 )
              PERFORM Acu-RPT-Report1f-REPORT-FOOTER
           END-IF
      * Normal Ending
           PERFORM Acu-Report1f-FILL-DETAIL-FOR-ZEBRA
           IF ( REPORT-FOOTER-PRINT-AFTER-PAGE = 0 )
              MOVE 1 TO SYS-ENDPRIMARYFD   
              COMPUTE CNT-PIXELS = ACU-PAGENUM-SYS * PAGE-HEIGHT OF
                 Report1f-HEIGHT - Report1f-PF-1-DEF-HEIGHT OF
                 Report1f-HEIGHT
              COMPUTE CNT-INPAGEPIXELS = PAGE-HEIGHT OF
                 Report1f-HEIGHT - Report1f-PF-1-DEF-HEIGHT OF
                 Report1f-HEIGHT
              MOVE PAGE-HEIGHT OF Report1f-HEIGHT TO CNT-INPAGEPIXELS
              PERFORM Acu-RPT-Report1f-PAGE-FOOTER
           END-IF
      * Check Report Footer
           IF ( REPORT-FOOTER-PRINT-AFTER-PAGE = 1 )
              PERFORM Acu-RPT-Report1f-REPORT-FOOTER
           END-IF
           .

       Acu-Report1f-HTML-PAGE-HEADER.
           MOVE ' <!-- Page DIV -->' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           STRING ' <DIV STYLE="POSITION:ABSOLUTE; TOP:', 
                  CNT-PIXELS, 
                  '; LEFT:',
                  Report1f-CLEFTOFFSET,
                  '; HEIGHT:', PAGE-HEIGHT OF Report1f-HEIGHT, 
                  '; WIDTH:', PAGE-WIDTH OF Report1f-HEIGHT,
                  ' ;">'
               DELIMITED BY SIZE INTO PRINT-BUF.
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-Report1f-HTML-PAGE-FOOTER.
           MOVE ' <!-- End(Page) --></DIV>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-RPT-Report1f-REPORT-HEADER.
      *    BeforePrint
           PERFORM Acu-Report1f-RH-1-PRINT-RTN
           ADD Report1f-RH-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
           ADD Report1f-RH-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
           MOVE 1 TO REPORT-HEADER-PRINT-BEFORE-PAGE
           PERFORM Acu-RPT-Report1f-PAGE-HEADER
           MOVE 0 TO REPORT-HEADER-PRINT-BEFORE-PAGE
      *    AfterPrint
           .

       Acu-RPT-Report1f-REPORT-FOOTER.
      *    BeforePrint
           MOVE Report1f-RF-1-DEF-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
           PERFORM Acu-RPT-Report1f-PAGE-FOOTER
           PERFORM Acu-Report1f-RF-1-PRINT-RTN
           ADD Report1f-RF-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
           ADD Report1f-RF-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
      *    AfterPrint
           .

       Acu-RPT-Report1f-PAGE-HEADER.
      *    BeforePrint
           IF ( CNT-INPAGEPIXELS < Report1f-PH-1-HEIGHT OF
              Report1f-HEIGHT ) OR ( REPORT-HEADER-PRINT-BEFORE-PAGE = 1
              )
              PERFORM Acu-Report1f-PH-1-PRINT-RTN
              ADD  Report1f-PH-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
              IF ( REPORT-HEADER-PRINT-BEFORE-PAGE = 1 )
                 ADD Report1f-PH-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              ELSE
                 MOVE Report1f-PH-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              END-IF
           END-IF
      *    AfterPrint
           .

       Acu-RPT-Report1f-PAGE-FOOTER.
      *    BeforePrint
           COMPUTE TMP-PIXELS = CNT-INPAGEPIXELS + CURRENT-HEIGHT +
              Report1f-PF-1-HEIGHT OF Report1f-HEIGHT
           IF TMP-PIXELS > PAGE-HEIGHT OF Report1f-HEIGHT
              COMPUTE CNT-PIXELS = ACU-PAGENUM-SYS * PAGE-HEIGHT OF
              Report1f-HEIGHT - Report1f-PF-1-HEIGHT OF Report1f-HEIGHT
              COMPUTE CNT-INPAGEPIXELS = PAGE-HEIGHT OF
              Report1f-HEIGHT - Report1f-PF-1-DEF-HEIGHT OF
              Report1f-HEIGHT
              PERFORM Acu-Report1f-PF-1-PRINT-RTN
              PERFORM Acu-Report1f-Page-BREAK-RTN
              MOVE Report1f-PF-1-DEF-HEIGHT OF Report1f-HEIGHT TO
              Report1f-PF-1-HEIGHT OF Report1f-HEIGHT
           END-IF
      *    AfterPrint         
           .

       Acu-RPT-Report1f-PAGE-FOOTER-OVERPAGE.
           MOVE 0 TO RPT-RETURN-VALUE
           COMPUTE TMP-PIXELS = CNT-INPAGEPIXELS + CURRENT-HEIGHT +
              Report1f-PF-1-HEIGHT OF Report1f-HEIGHT
           IF TMP-PIXELS + 1 >= PAGE-HEIGHT OF Report1f-HEIGHT
              MOVE 1 TO RPT-RETURN-VALUE
           END-IF
           .

       Acu-RPT-Report1f-DETAIL.
           PERFORM Acu-Report1f-Dtl-1-DETAIL-PRINT.
           .

       Acu-RPT-Report1f-GROUP-HEADER.
      * Cause there're possibly several Group.
           PERFORM Acu-Report1f-GH-1-GROUP-PRINT.
           PERFORM Acu-Report1f-GH-2-GROUP-PRINT.
           .

       Acu-RPT-Report1f-GROUP-FOOTER.
           PERFORM Acu-Report1f-GF-2-GROUP-PRINT.
           .

       Acu-RPT-Report1f-GROUP-MUSTDSP.
           MOVE 1 TO Report1f-GF-1-MUST-DISPLAY
           MOVE 1 TO Report1f-GF-2-MUST-DISPLAY
           .

       Acu-RPT-Report1f-GROUP-NOMUSTDSP.
           MOVE 0 TO Report1f-GF-1-MUST-DISPLAY
           MOVE 0 TO Report1f-GF-2-MUST-DISPLAY
           .

       Acu-RPT-Report1f-ALL-RESET.
      * Group Reset
           PERFORM Acu-Report1f-Report1f-GH-1-RESET
           PERFORM Acu-Report1f-Report1f-GH-2-RESET
           PERFORM Acu-Report1f-Report1f-GF-1-RESET
           PERFORM Acu-Report1f-Report1f-GF-2-RESET
           .

       Acu-RPT-Report1f-DO-PRINT-RTN.
           MOVE 1 TO TMP-PRINTORNOT
      * Check Group Footer
           PERFORM Acu-RPT-Report1f-GROUP-FOOTER
      * Init Detail's Height
           MOVE Report1f-Dtl-1-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
           PERFORM Acu-RPT-Report1f-PAGE-FOOTER
           PERFORM Acu-RPT-Report1f-PAGE-HEADER
      * Check Group Header
           PERFORM Acu-RPT-Report1f-GROUP-HEADER
      * Detail
           PERFORM Acu-RPT-Report1f-DETAIL
           .

       Acu-Report1f-Page-BREAK-RTN.
           IF CURRENT-COL < NTOPCOLUMNNUM
              COMPUTE CNT-PIXELS = (ACU-PAGENUM-SYS - 1) 
                 * PAGE-HEIGHT OF Report1f-HEIGHT
              COMPUTE Report1f-CLEFTOFFSET = 
                 Report1f-CLEFTOFFSET + DETAIL-WIDTH
              MOVE 0 TO CNT-INPAGEPIXELS
              ADD 1 TO CURRENT-COL
           ELSE
              COMPUTE CNT-PIXELS = ACU-PAGENUM-SYS 
                 * PAGE-HEIGHT OF Report1f-HEIGHT
              MOVE 0 TO Report1f-CLEFTOFFSET
              MOVE 0 TO CNT-INPAGEPIXELS
              ADD 1 TO ACU-PAGENUM-SYS
              ADD 1 TO ACU-PAGENUM
              MOVE 1 TO CURRENT-COL
           END-IF
           PERFORM Acu-Report1f-HTML-PAGE-FOOTER
           IF SYS-ENDPRIMARYFD = 0
              PERFORM Acu-Report1f-HTML-PAGE-HEADER
           END-IF
           .           

       Acu-Report1f-FILL-DETAIL-FOR-ZEBRA.
           .

       Acu-Report1f-ZEBRA-COLOR-CHANGE.
           .
      
       Acu-Report1f-Dtl-1-DETAIL-PRINT.
           MOVE 816 TO DETAIL-WIDTH
           PERFORM Report1f-Dtl-1-BeforePrint
           MOVE Report1f-Dtl-1-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
           PERFORM Acu-RPT-Report1f-PAGE-FOOTER
           PERFORM Acu-RPT-Report1f-PAGE-HEADER
           PERFORM Acu-Report1f-Dtl-1-PRINT-RTN
           ADD Report1f-Dtl-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
           ADD Report1f-Dtl-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
           MOVE Report1f-Dtl-1-DEF-HEIGHT OF Report1f-HEIGHT TO
              Report1f-Dtl-1-HEIGHT OF Report1f-HEIGHT
           PERFORM Acu-Report1f-ZEBRA-COLOR-CHANGE
      *    AfterPrint
           .

       Acu-Report1f-GH-1-GROUP-PRINT.
      * GROUP-HEADER : Report1f-GH-1
      *    BeforePrint
           IF Report1f-GH-1-GROUPBY-BUF NOT = Sales-State OF Salesdata
              MOVE Report1f-GH-1-DEF-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
      * Skip Page Before Print : True
              MOVE PAGE-HEIGHT OF Report1f-HEIGHT TO CURRENT-HEIGHT
              PERFORM Acu-RPT-Report1f-PAGE-FOOTER
              PERFORM Acu-RPT-Report1f-PAGE-HEADER
              PERFORM Acu-Report1f-GH-1-PRINT-RTN
              ADD Report1f-GH-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
              ADD Report1f-GH-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              PERFORM Acu-Report1f-Report1f-GH-2-RESET
              MOVE Sales-State OF Salesdata TO Report1f-GH-1-GROUPBY-BUF
           END-IF
           PERFORM Report1f-GH-1-AfterPrint
           .

       Acu-Report1f-Report1f-GH-1-RESET.
           MOVE HIGH-VALUE TO Report1f-GH-1-GROUPBY-BUF
           PERFORM Acu-Report1f-Report1f-GH-2-RESET
           .

       Acu-Report1f-GH-2-GROUP-PRINT.
      * GROUP-HEADER : Report1f-GH-2
      *    BeforePrint
           IF Report1f-GH-2-GROUPBY-BUF NOT = Sales-Branch-No OF
              Salesdata
              MOVE Report1f-GH-2-DEF-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
              PERFORM Acu-RPT-Report1f-PAGE-FOOTER
              PERFORM Acu-RPT-Report1f-PAGE-HEADER
              PERFORM Acu-Report1f-GH-2-PRINT-RTN
              ADD Report1f-GH-2-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
              ADD Report1f-GH-2-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              MOVE Sales-Branch-No OF Salesdata TO
              Report1f-GH-2-GROUPBY-BUF
           END-IF
           PERFORM Report1f-GH-2-AfterPrint
           .

       Acu-Report1f-Report1f-GH-2-RESET.
           MOVE HIGH-VALUE TO Report1f-GH-2-GROUPBY-BUF
           .

       Acu-Report1f-GF-1-GROUP-PRINT.
      *    BeforePrint
      * Group-Footer : Report1f-GF-1
           IF ((Report1f-GF-1-GROUPBY-BUF NOT = Sales-Branch-No OF
              Salesdata) AND (Report1f-GF-1-GROUPBY-BUF NOT =
              HIGH-VALUE)) OR 
              (Report1f-GF-1-MUST-DISPLAY NOT = 0)
      * Group-Footer
              MOVE Report1f-GF-1-DEF-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
              PERFORM Acu-RPT-Report1f-PAGE-FOOTER
              PERFORM Acu-RPT-Report1f-PAGE-HEADER
              PERFORM Acu-Report1f-GF-1-PRINT-RTN
      * End of Group-Footer Print
              ADD Report1f-GF-1-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
              ADD Report1f-GF-1-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              MOVE Sales-Branch-No OF Salesdata TO
              Report1f-GF-1-GROUPBY-BUF
           END-IF
           MOVE Sales-Branch-No OF Salesdata TO
              Report1f-GF-1-GROUPBY-BUF
      *    AfterPrint
           .

       Acu-Report1f-Report1f-GF-1-RESET.
           MOVE HIGH-VALUE TO Report1f-GF-1-GROUPBY-BUF
           MOVE 0 TO Report1f-GF-1-MUST-DISPLAY
           .

       Acu-Report1f-GF-2-GROUP-PRINT.
      *    BeforePrint
      * Group-Footer : Report1f-GF-2
           IF ((Report1f-GF-2-GROUPBY-BUF NOT = Sales-State OF
              Salesdata) AND (Report1f-GF-2-GROUPBY-BUF NOT =
              HIGH-VALUE)) OR 
              (Report1f-GF-2-MUST-DISPLAY NOT = 0)
              PERFORM Acu-RPT-Report1f-GROUP-MUSTDSP
              PERFORM Acu-Report1f-GF-1-GROUP-PRINT
              PERFORM Acu-RPT-Report1f-GROUP-NOMUSTDSP
      * Group-Footer
              MOVE Report1f-GF-2-DEF-HEIGHT OF Report1f-HEIGHT TO
              CURRENT-HEIGHT
              PERFORM Acu-RPT-Report1f-PAGE-FOOTER
              PERFORM Acu-RPT-Report1f-PAGE-HEADER
              PERFORM Acu-Report1f-GF-2-PRINT-RTN
      * End of Group-Footer Print
              ADD Report1f-GF-2-HEIGHT OF Report1f-HEIGHT TO CNT-PIXELS
              ADD Report1f-GF-2-HEIGHT OF Report1f-HEIGHT TO
              CNT-INPAGEPIXELS
              MOVE Sales-State OF Salesdata TO Report1f-GF-2-GROUPBY-BUF
           ELSE
              PERFORM Acu-Report1f-GF-1-GROUP-PRINT
           END-IF
           MOVE Sales-State OF Salesdata TO Report1f-GF-2-GROUPBY-BUF
      *    AfterPrint
           .

       Acu-Report1f-Report1f-GF-2-RESET.
           MOVE HIGH-VALUE TO Report1f-GF-2-GROUPBY-BUF
           MOVE 0 TO Report1f-GF-2-MUST-DISPLAY
           PERFORM Acu-Report1f-Report1f-GF-1-RESET
           .

       Acu-Report1f-RwTb-1-TABHEADER.
           MOVE '      <!-- Table (Report1f-RwTb-1) -->' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE 
              '      <DIV CLASS="Report1f-RwTb-1-TABLEDIV" STYLE="TOP:10
      -       ';">' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE 
              '      <TABLE cellspacing=1 cellpadding=1 BORDER=0  CLASS=
      -       '"Report1f-RwTb-1-TAB">' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-Report1f-RwTb-1-TABFOOTER.
           MOVE "         </TABLE></P></DIV>" TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-Report1f-RwTb-1-CELL-COLOR.
           MOVE SPACES TO DUMY-LINE
           MOVE SPACES TO Report1f-RwTb-1-FGCOLOR,
              Report1f-RwTb-1-BGCOLOR
      * Check Column Color
      * Check Row Color
      * Check Cell Color
           MOVE 1 TO ACU-IDX3
           IF Report1f-RwTb-1-FGCOLOR NOT = SPACES OR
              Report1f-RwTb-1-BGCOLOR NOT = SPACES
              MOVE SPACES TO Report1f-RwTb-1-DUMY-LINE
              STRING 'COLOR:', Report1f-RwTb-1-FGCOLOR,
                     ';BACKGROUND-COLOR:', Report1f-RwTb-1-BGCOLOR, ';'
                 DELIMITED BY SIZE INTO DUMY-LINE
              MOVE 0 TO ACU-IDX3
              INSPECT DUMY-LINE TALLYING ACU-IDX3 FOR TRAILING SPACE
              COMPUTE ACU-IDX3 = 132 - ACU-IDX3
           END-IF
           .

       Acu-Report1f-RwTb-1-CELL-FONT.
           MOVE DUMY-LINE TO Report1f-RwTb-1-DUMY-LINE
           MOVE 0      TO Report1f-RwTb-1-FONTSIZE
           MOVE SPACES TO Report1f-RwTb-1-FONT
           MOVE "<STRONG>" TO Report1f-RwTb-1-FONTPRE
           MOVE "</STRONG>" TO Report1f-RwTb-1-FONTAFT
      * Check Column Font
      * Check Row Font
      * Check Cell Font
           MOVE ACU-IDX3 TO Report1f-RwTb-1-IDX
           IF Report1f-RwTb-1-FONTSIZE NOT = 0
              MOVE SPACES TO Report1f-RwTb-1-DUMY-LINE
              MOVE 0 TO Report1f-RwTb-1-IDX
              INSPECT Report1f-RwTb-1-FONT TALLYING
              Report1f-RwTb-1-IDX FOR TRAILING SPACE
              STRING DUMY-LINE(1:ACU-IDX3), 'FONT-SIZE:',
              Report1f-RwTb-1-FONTSIZE,
           	       'pt;FONT-FAMILY:',
              	       Report1f-RwTb-1-FONT(1:30 - Report1f-RwTb-1-IDX),
                     ';'
                 DELIMITED BY SIZE INTO Report1f-RwTb-1-DUMY-LINE
              MOVE 0 TO Report1f-RwTb-1-IDX
              INSPECT Report1f-RwTb-1-DUMY-LINE TALLYING
              Report1f-RwTb-1-IDX FOR TRAILING SPACE
              COMPUTE Report1f-RwTb-1-IDX = 132 - Report1f-RwTb-1-IDX
           END-IF
           .

       Acu-Report1f-RwTb-1-CELL-ALIGN.
           MOVE SPACES TO Report1f-RwTb-1-ALIGN,
              Report1f-RwTb-1-VALIGN, DUMY-LINE
      * Check Column Align
      * Check Row Align
      * Check Row Align
           IF ACU-IDX1 = 1 AND ACU-IDX2 = 1
              MOVE 'VALIGN="Middle"' TO Report1f-RwTb-1-VALIGN
              MOVE 'ALIGN="Center"' TO Report1f-RwTb-1-ALIGN
           END-IF
           IF ACU-IDX1 = 1 AND ACU-IDX2 = 2
              MOVE 'VALIGN="Middle"' TO Report1f-RwTb-1-VALIGN
              MOVE 'ALIGN="Center"' TO Report1f-RwTb-1-ALIGN
           END-IF
           MOVE 1 TO ACU-IDX3
           STRING ' ', Report1f-RwTb-1-ALIGN,
                  ' ', Report1f-RwTb-1-VALIGN
              DELIMITED BY SIZE INTO DUMY-LINE
           IF DUMY-LINE NOT = SPACES
              MOVE 0 TO ACU-IDX3
              INSPECT DUMY-LINE TALLYING ACU-IDX3 FOR TRAILING SPACE
              COMPUTE ACU-IDX3 = 132 - ACU-IDX3
           END-IF
           .

       Acu-Report1f-RwTb-1-TABLECELL.
           PERFORM Acu-Report1f-RwTb-1-CELL-COLOR
           PERFORM Acu-Report1f-RwTb-1-CELL-FONT
           PERFORM Acu-Report1f-RwTb-1-CELL-ALIGN
           EVALUATE ACU-IDX1 ALSO ACU-IDX2
           WHEN 1 ALSO 1
              MOVE Report1f-RwTb-1-ROWHEIGHT TO
              Report1f-RwTb-1-TMP-HEIGHT
              STRING '            <TD nowrap height=',
                     Report1f-RwTb-1-TMP-HEIGHT, 
                     ' CLASS="Report1f-RwTb-1-COL1"  STYLE="BORDER-top:0
      -       'px; "',
                     DUMY-LINE(1:ACU-IDX3),
                     ' STYLE="',
                     Report1f-RwTb-1-DUMY-LINE(1:Report1f-RwTb-1-IDX),
                     '"><DIV STYLE="width:96; OVERFLOW:HIDDEN;">',
                     Report1f-RwTb-1-FONTPRE
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              STRING "State:"
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              STRING '            ', Report1f-RwTb-1-FONTAFT, '</DIV></T
      -       'D>' DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
           WHEN 1 ALSO 2
              MOVE Report1f-RwTb-1-ROWHEIGHT TO
              Report1f-RwTb-1-TMP-HEIGHT
              STRING '            <TD nowrap height=',
                     Report1f-RwTb-1-TMP-HEIGHT, 
                     ' CLASS="Report1f-RwTb-1-COL2"  STYLE="BORDER-top:0
      -       'px; "',
                     DUMY-LINE(1:ACU-IDX3),
                     ' STYLE="',
                     Report1f-RwTb-1-DUMY-LINE(1:Report1f-RwTb-1-IDX),
                     '"><DIV STYLE="width:96; OVERFLOW:HIDDEN;">',
                     Report1f-RwTb-1-FONTPRE
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              IF Sales-State OF Salesdata(1:) = SPACES
                 STRING '&nbsp;'
              	  DELIMITED BY SIZE INTO PRINT-BUF
              ELSE
                 STRING Sales-State OF Salesdata
                    DELIMITED BY SIZE INTO PRINT-BUF
              END-IF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              STRING '            ', Report1f-RwTb-1-FONTAFT, '</DIV></T
      -       'D>' DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
           WHEN 1 ALSO 3
              MOVE Report1f-RwTb-1-ROWHEIGHT TO
              Report1f-RwTb-1-TMP-HEIGHT
              STRING '            <TD nowrap height=',
                     Report1f-RwTb-1-TMP-HEIGHT, 
                     ' CLASS="Report1f-RwTb-1-COL3"  STYLE="BORDER-top:0
      -       'px; BORDER-right:0px; "',
                     DUMY-LINE(1:ACU-IDX3),
                     ' STYLE="',
                     Report1f-RwTb-1-DUMY-LINE(1:Report1f-RwTb-1-IDX),
                     '"><DIV STYLE="width:96; OVERFLOW:HIDDEN;">',
                     Report1f-RwTb-1-FONTPRE
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              STRING '<IMG SRC="',
                     State-Jpeg-Name,
                     '" width=', Report1f-RwTb-1-COLWIDTH,
                     ' height=', Report1f-RwTb-1-TMP-HEIGHT, '>'
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              STRING '            ', Report1f-RwTb-1-FONTAFT, '</DIV></T
      -       'D>' DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
           END-EVALUATE
           IF PRINT-BUF NOT = SPACES
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
           END-IF
           .

      * RPT.Table : Report1f-RwTb-1
       Acu-Report1f-RwTb-1-TABBODY.
           PERFORM Acu-Report1f-RwTb-1-TABHEADER
           PERFORM VARYING ACU-IDX1 FROM 1 BY 1 UNTIL ACU-IDX1 > 1
      * Row-Height
              EVALUATE ACU-IDX1
                 WHEN 1
                    MOVE 57 TO Report1f-RwTb-1-ROWHEIGHT
              END-EVALUATE
      * Row <TR>
              STRING '         <TR>'
                 DELIMITED BY SIZE INTO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
              PERFORM VARYING ACU-IDX2 FROM 1 BY 1 UNTIL ACU-IDX2 > 3
                 EVALUATE ACU-IDX2
                 WHEN 1
                    MOVE 96 TO Report1f-RwTb-1-COLWIDTH
                 WHEN 2
                    MOVE 96 TO Report1f-RwTb-1-COLWIDTH
                 WHEN 3
                    MOVE 96 TO Report1f-RwTb-1-COLWIDTH
                 END-EVALUATE
                 PERFORM Acu-Report1f-RwTb-1-TABLECELL
              END-PERFORM
      * End of Row </TR>
              MOVE '         </TR>' TO PRINT-BUF
              WRITE PRINTF-R FROM PRINT-BUF AFTER 001
              INITIALIZE PRINT-BUF
           END-PERFORM
           PERFORM Acu-Report1f-RwTb-1-TABFOOTER
           .

       Acu-Report1f-RH-1-SEC-HEAD.
