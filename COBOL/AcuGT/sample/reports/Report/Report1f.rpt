           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '         <P>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           STRING '<IMG SRC="FILE:C:\Acucorp\Acucbl720\AcuGT\sample\repo
      -       'rts\Data\acucorp.bmp" STYLE="POSITION:ABSOLUTE; TOP:0; LE
      -       'FT:0; WIDTH:648; HEIGHT:97; " >'
