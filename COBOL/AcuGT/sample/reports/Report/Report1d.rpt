           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           IF PR-Quota = 1
              STRING '<INPUT TYPE="CHECKBOX" CHECKED >',
                     ' '
                 DELIMITED BY SIZE INTO PRINT-BUF
           ELSE
              STRING '<INPUT TYPE="CHECKBOX">',
                     ' '
                 DELIMITED BY SIZE INTO PRINT-BUF
           END-IF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '         </STRONG></P></DIV>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
      *    AfterPrint
           PERFORM Acu-Report1d-Dtl-1-SEC-FOOT
           .

       Acu-Report1d-PF-1-SEC-HEAD.
           MOVE '   <!-- Section (Report1d-PF-1) -->' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           COMPUTE TMP-PIXELS = CNT-INPAGEPIXELS - HIGH-OFFSET-IN
           STRING '   <DIV STYLE="',
                  'POSITION:ABSOLUTE; TOP:', 
                  TMP-PIXELS,
                  '; LEFT:0',
                  '; WIDTH:857; ',
                  'FONT-SIZE:9pt; ',
                  'FONT-FAMILY:MS Sans Serif; ',
                  'COLOR:#000000; ',
                  'BACKGROUND-COLOR:#FFFFFF; ',
                  '">'
              DELIMITED BY SIZE INTO PRINT-BUF.
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-Report1d-PF-1-SEC-FOOT.
           MOVE '      <!-- Ending DIV : to fill the Div Height -->' TO
              PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           STRING '      <DIV STYLE="POSITION:RELATIVE; HEIGHT:',
                  Report1d-PF-1-HEIGHT OF Report1d-HEIGHT, 
                  ';"> </DIV>'
              DELIMITED BY SIZE INTO PRINT-BUF.
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '   <!-- End(Section) --></DIV>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           .

       Acu-Report1d-PF-1-PRINT-RTN.
           MOVE 0 TO SECTION-HIGH-STACK
           MOVE Report1d-PF-1-DEF-HEIGHT TO Report1d-PF-1-HEIGHT
           PERFORM Acu-Report1d-PF-1-SEC-HEAD
      * RPT.Date-Time : Report1d-RwDtTm-1
      *    BeforePrint
           MOVE '      <!-- Date-Time (Report1d-RwDtTm-1) -->' TO
              PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '      <DIV CLASS="Report1d-RwDtTm-1" >' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '         <P><STRONG>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           PERFORM Acu-Report1d-RwDtTm-1-FORMAT-CONVERT
           MOVE WRK-DATE-REF OF Report1d-RwDtTm-1-DATEBUF TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '         </STRONG></P></DIV>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
      *    AfterPrint
           PERFORM Acu-Report1d-PF-1-SEC-FOOT
           .

       Acu-Report1d-RF-1-SEC-HEAD.
           MOVE '   <!-- Section (Report1d-RF-1) -->' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           COMPUTE TMP-PIXELS = CNT-INPAGEPIXELS - HIGH-OFFSET-IN
           STRING '   <DIV STYLE="',
                  'POSITION:ABSOLUTE; TOP:', 
                  TMP-PIXELS,
                  '; LEFT:0',
                  '; WIDTH:857; ',
                  'FONT-SIZE:9pt; ',
                  'FONT-FAMILY:MS Sans Serif; ',
                  'COLOR:#000000; ',
                  'BACKGROUND-COLOR:#FFFFFF; ',
                  '">'
              DELIMITED BY SIZE INTO PRINT-BUF.
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           .

       Acu-Report1d-RF-1-SEC-FOOT.
           MOVE '      <!-- Ending DIV : to fill the Div Height -->' TO
              PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           STRING '      <DIV STYLE="POSITION:RELATIVE; HEIGHT:',
                  Report1d-RF-1-HEIGHT OF Report1d-HEIGHT, 
                  ';"> </DIV>'
              DELIMITED BY SIZE INTO PRINT-BUF.
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '   <!-- End(Section) --></DIV>' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           .

       Acu-Report1d-RF-1-PRINT-RTN.
           MOVE 0 TO SECTION-HIGH-STACK
           MOVE Report1d-RF-1-DEF-HEIGHT TO Report1d-RF-1-HEIGHT
           PERFORM Acu-Report1d-RF-1-SEC-HEAD
      * RPT.Label : Report1d-RwLa-10
      *    BeforePrint
           MOVE '      <!-- Label (Report1d-RwLa-10) -->' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           MOVE '      <DIV CLASS="Report1d-RwLa-10" >' TO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
           STRING '         <P><STRONG>',
                  '***&nbsp;End&nbsp;Sales&nbsp;Report&nbsp;***',
                  '</STRONG></P></DIV>'
              DELIMITED BY SIZE INTO PRINT-BUF
           WRITE PRINTF-R FROM PRINT-BUF AFTER 001
           INITIALIZE PRINT-BUF
      *    AfterPrint
           PERFORM Acu-Report1d-RF-1-SEC-FOOT
           .

      * Date/Time Format Convert
       Acu-Report1d-RwDtTm-1-FORMAT-CONVERT.
           ACCEPT WRK-DATE OF Report1d-RwDtTm-1-DATEBUF FROM
              CENTURY-DATE
           MOVE WRK-YEAR  OF Report1d-RwDtTm-1-DATEBUF TO
              WRK-REF-YEAR  OF Report1d-RwDtTm-1-DATEBUF
           MOVE WRK-MONTH OF Report1d-RwDtTm-1-DATEBUF TO
              WRK-REF-MONTH OF Report1d-RwDtTm-1-DATEBUF
           MOVE WRK-DAY   OF Report1d-RwDtTm-1-DATEBUF TO WRK-REF-DAY
                OF Report1d-RwDtTm-1-DATEBUF
           ACCEPT WRK-TIME OF Report1d-RwDtTm-1-DATEBUF FROM TIME
           .


       Acu-PRINTF-OPEN-ROUTINE.
           OPEN   OUTPUT               PRINTF WITH NO REWIND.
           MOVE   STAT-PRINTF          TO    STAT-PRINTF.
           IF     NOT VALID-PRINTF
                  MOVE   "PRINTF"      TO   ACU-ERR-FILE.

       Acu-PRINTF-CLOSE-ROUTINE.
           CLOSE       PRINTF.
           
