    <h2 weight="10"/>
    <caption weight="10"/>
    <h3 weight="7"/>
    <th weight="5"/>
    <h4 weight="5"/>
    <h5 weight="4"/>
    <h6 weight="3"/>
    <h7 weight="2"/>
